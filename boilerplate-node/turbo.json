{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "clean": {
      "cache": false
    },
    "build": {
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "dependsOn": ["^build"],
      "outputs": [
        "build/**",
        ".vercel/**",
        "dist/**",
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "make-migration": {
      "cache": false,
      "persistent": true
    },
    "migrate": {
      "cache": false,
      "persistent": true
    },
    "make-seed": {
      "cache": false,
      "persistent": true
    },
    "run-seed": {
      "cache": false,
      "persistent": true
    },
    "run-specific-seed": {
      "cache": false,
      "persistent": true
    },
    "rollback": {
      "cache": false,
      "persistent": true
    },
    "clear-db": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^build"]
    },
    "generate-keys": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "outputs": [],
      "dependsOn": []
    },
    "lint": {
      "dependsOn": ["^build", "^lint"]
    },
    "check-types": {
      "dependsOn": ["^build", "^check-types"]
    }
  }
}
